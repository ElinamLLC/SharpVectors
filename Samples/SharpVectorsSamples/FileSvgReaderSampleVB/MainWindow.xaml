<Window x:Class="MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Svg Reader Test Sample - VB" Height="460" Width="620" WindowStartupLocation="CenterScreen" Name="Window1">
    <DockPanel LastChildFill="True">
        <TabControl>
            <TabItem Header="Drawing">
                <Image Name="svgImage" Stretch="Uniform"/>
            </TabItem>
            <TabItem Header="XAML">
                <TextBox Name="svgBox" IsReadOnly="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"/>
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
