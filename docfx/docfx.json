{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SharpVectorCore/SharpVectors.Core.csproj",
            "SharpVectorCss/SharpVectors.Css.csproj",
            "SharpVectorDom/SharpVectors.Dom.csproj",
            "SharpVectorModel/SharpVectors.Model.csproj",
            "SharpVectorRuntimeWpf/SharpVectors.Runtime.Wpf.csproj",
            "SharpVectorRenderingWpf/SharpVectors.Rendering.Wpf.csproj",
            "SharpVectorConvertersWpf/SharpVectors.Converters.Wpf.csproj"
          ],
	  "src": "../Source/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
	  "filter": "docfxfilter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
	  "logo.svg",
          "favicon.ico",
          "styles/*.css",
          "styles/*.js"	  
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md",
		  "namespaces/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
	"xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_appName": "SharpVectors",
      "_appTitle": "SharpVectors Documentation",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",	  
      "_enableDiagrams": true,
      "_enableSearch": true,
      "_enableNewTab": false,
      "_disableAffix": true,
      "_appFooter": "Elinam LLC, Toyota City - Japan<br/><small>Generated by DocFX</small>",
      "_disableSideFilter": true,
      "_gitContribute": {
        "repo": "https://github.com/ElinamLLC/SharpVectors",
		"branch": "master"
      },
      "_gitUrlPattern": "github"
    },
    "template": [ "default", "templates/discordfx", "mermaid" ],
    "properties": {
       "TargetFramework": "net6.0-windows"
	},
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}